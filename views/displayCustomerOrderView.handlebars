<div>
    <div class="table-responsive">
        <table class="table table-condensed">
            <thead>
                <tr>
                    <th>Order Date</th>
                    <th>Order Quantity</th>
                    <th>Order Product</th>
                </tr>
            </thead>
            <tbody>
                {{#each data}}
                <tr>
                    <td>{{this.orderDate}}</td>
                    <td>{{this.orderQuantity}}</td>
                    <td>{{this.orderProduct.productName}}</td>
                    </td>
                </tr>
            {{/each}}
            </tbody>
             <tfoot>
                <tr><td>
                    <button class="btn btn-primary" onclick="continueShopping()">Shop</button>
                </td></tr>
            </tfoot>
        </table>
    </div>
</div>

